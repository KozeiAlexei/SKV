@using SKV.PL.Helpers
@using System.Linq.Expressions;

@ClientSide.TableSettings.CreateTableSettingsFactory("SKV", "UserTableSettingsFactory", new Expression<Func<ClientSide.TableSettings, string>>[]
{
     s => s.CreateColumn("UserName", Resources.Resource.UserName, 20, true, ColumnDataType.Text, false),
     s => s.CreateColumn("Email", Resources.Resource.Email, 20, true, ColumnDataType.Text, false),
     s => s.CreateColumn("PhoneNumber", Resources.Resource.PhoneNumber, 20, true, ColumnDataType.Text, false),
     s => s.CreateColumn("Profile.Name", Resources.Resource.UserInitials, 20, true, ColumnDataType.Text, false),
     s => s.CreateColumn("Actions", Resources.Resource.Actions, 20, false, ColumnDataType.Actions, false),
});