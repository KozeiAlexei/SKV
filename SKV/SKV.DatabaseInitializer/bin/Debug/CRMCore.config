<Configuration>
  <Angular.Main>
    <add key="ApplicationName" value="SKV" />
    <add key="DynamicTableControllerAs" value="ctrl" />
  </Angular.Main>

  <Angular.ModalNames>
    <add key="UserProfileModalName" value="UserProfileModal" />
    <add key="UserCreatingModalName" value="UserCreatingModal" />

    <add key="RoleProfileModalName" value="RoleProfileModal" />
    <add key="RoleCreatingModalName" value="RoleCreatingModal" />

    <add key="ConfirmationModalName" value="ConfirmationModal" />
  </Angular.ModalNames>

  <Angular.FactoryNames>
    <add key="UserTableSettingsFactory" value="UserTableSettingsFactory" />
    <add key="RoleTableSettingsFactory" value="RoleTableSettingsFactory" />
  </Angular.FactoryNames>

  <Angular.ControllerAs>
    <add key="UserManagerControllerAs" value="umCtrl" />
    <add key="RoleManagerControllerAs" value="rmCtrl" />
  </Angular.ControllerAs>

  <Angular.TableActionIds>
    <add key="ViewActionId" value="ViewAction" />
    <add key="DeleteActionId" value="DeleteAction" />
  </Angular.TableActionIds>

  <Angular.ControllerNames>
    <add key="UserManagerController" value="UserManagerController" />
    <add key="RoleManagerController" value="RoleManagerController" />
  </Angular.ControllerNames>

  <CRM.Main>
    <add key="DefaultTablePageSize" value="3" />
    <add key="ClientSideTemplatesPath" value="~/Views/ComponentTemplates/" />
  </CRM.Main>
</Configuration>